{
    "name": "vetnext/front",
    "description": "Views for vet store",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vaidotas Lipskas",
            "email": "vaidotaslipskas@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/ui": "^4.0@dev",
        "laravel/breeze": "2.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "laravel/laravel": "8.x-dev"
    },
    "autoload": {
        "psr-4": {
            "VetNext\\Front\\": "src",
            "VetNext\\Front\\Tests\\": "tests",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VetNext\\Front\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VetNext\\Front\\Providers\\ViewsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    }
}
